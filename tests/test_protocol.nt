test_protocol_add_footnotes:
  -
    footnotes_new: []
    footnotes_before: {}
    footnotes_after: {}
    formatted_ids: ''
  -
    footnotes_new:
      - 'Footnote 1'
    footnotes_before: {}
    footnotes_after:
      1: 'Footnote 1'
    formatted_ids: ' [1]'
  -
    footnotes_new:
      - 'Footnote 1'
      - 'Footnote 2'
    footnotes_before: {}
    footnotes_after:
      1: 'Footnote 1'
      2: 'Footnote 2'
    formatted_ids: ' [1,2]'
  -
    footnotes_new:
      - 'Footnote 3'
    footnotes_before:
      2: 'Footnote 2'
    footnotes_after:
      2: 'Footnote 2'
      3: 'Footnote 3'
    formatted_ids: ' [3]'

test_protocol_insert_footnotes:
  -
    steps_before: []
    footnotes_before: {}
    footnotes_new: []
    steps_after: []
    footnotes_after: {}
  -
    steps_before:
      - 'Step 1:'
    footnotes_before: {}
    footnotes_new:
      - 'Footnote 1'
    steps_after:
      - 'Step 1 [1]:'
    footnotes_after:
      1: 'Footnote 1'
  -
    steps_before:
      - 'Step 1:'
      - 'Step 2:'
    footnotes_before: {}
    footnotes_new:
      - 'Footnote 1'
    steps_after:
      - 'Step 1:'
      - 'Step 2 [1]:'
    footnotes_after:
      1: 'Footnote 1'
  -
    steps_before:
      - 'Step 1:'
    footnotes_before: {}
    footnotes_new:
      - 'Footnote 1'
      - 'Footnote 2'
    steps_after:
      - 'Step 1 [1,2]:'
    footnotes_after:
      1: 'Footnote 1'
      2: 'Footnote 2'
  -
    steps_before:
      - 'Step 1:'
    footnotes_before:
      1: 'Footnote 1'
    footnotes_new:
      - 'Footnote 2'
    steps_after:
      - 'Step 1 [2]:'
    footnotes_after:
      1: 'Footnote 1'
      2: 'Footnote 2'
  -
    steps_before:
      - 'Step 1:'
      - 'Step 2:'
    footnotes_before: {}
    footnotes_new:
      - 'Footnote 1'
    steps_after:
      - 'Step 1:'
      - 'Step 2 [1]:'
    footnotes_after:
      1: 'Footnote 1'
  -
    steps_before:
      - 'Step 1'
    footnotes_before: {}
    pattern: \d
    footnotes_new:
      - 'Footnote 1'
    steps_after:
      - 'Step 1 [1]'
    footnotes_after:
      1: 'Footnote 1'
  -
    steps_before:
      - pl('Step 1:')
    footnotes_before: {}
    footnotes_new:
      - 'Footnote 1'
    steps_after:
      - pl('Step 1 [1]:')
    footnotes_after:
      1: 'Footnote 1'
  -
    # Although we go through the steps backwards, we go through each individual 
    # steep forward.
    steps_before:
      - 'Sentence 1.  Sentence 2.'
      - 'Sentence 3.  Sentence 4.'
    footnotes_before: {}
    footnotes_new:
      - 'Footnote 1'
    steps_after:
      - 'Sentence 1.  Sentence 2.'
      - 'Sentence 3 [1].  Sentence 4.'
    footnotes_after:
      1: 'Footnote 1'
  -
    # Although we go through the steps backwards, we go through each individual 
    # steep forward.
    steps_before:
      - pl('Paragraph 1:', 'Paragraph 2:')
      - pl('Paragraph 3:', 'Paragraph 4:')
    footnotes_before: {}
    footnotes_new:
      - 'Footnote 1'
    steps_after:
      - pl('Paragraph 1:', 'Paragraph 2:')
      - pl('Paragraph 3 [1]:', 'Paragraph 4:')
    footnotes_after:
      1: 'Footnote 1'

  -
    id: err
    steps_before: []
    footnotes_before: {}
    footnotes_new:
      - 'Footnote 1'
    error:
      type: ValueError
      message: pattern '\(\?=\[\.:\]\)' not found in protocol
  -
    id: err
    steps_before:
      - 'Step 1'
    footnotes_before: {}
    footnotes_new:
      - 'Footnote 1'
    error:
      type: ValueError
      message: pattern '\(\?=\[\.:\]\)' not found in protocol

test_protocol_renumber_footnotes:
