      - pl('Step 1', ul('Substep 1'))
    expected:
      > 1. Step 1
      >
      >    - Substep 1
  -
    steps:
      - 'Step 1:\nLine wrap'
    expected:
      > 1. Step 1: Line wrap
  -
    steps:
      - pre('Step 1\nLine break')
    expected:
      > 1. Step 1
      >    Line break
  -
    steps:
      - pl('Step 1:\nLine wrap', ul('Substep 1:\nAnother line wrap'))
    expected:
      > 1. Step 1: Line wrap
      >
      >    - Substep 1: Another line wrap
  -
    steps:
