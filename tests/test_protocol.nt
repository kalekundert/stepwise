    footnotes_after : {1: 'Footnote 1'}
  -
    new_ids: 2
    steps_before: ['[1]']
    steps_after : ['[2]']
    footnotes_before: {1: 'Footnote 1'}
    footnotes_after : {2: 'Footnote 1'}
  -
    new_ids: 1
    steps_before: ['[2]']
    steps_after : ['[1]']
    footnotes_before: {2: 'Footnote 1'}
    footnotes_after : {1: 'Footnote 1'}
  -
