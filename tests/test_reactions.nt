        >         ],
        >         footer=["", "6.00 µL"],
        >         align=list("<>"),
        >       ),
        >     ),
        >     br='\n\n',
        >   ),
        >   ul('d', 'e'),
        > )
  -
    id: 3-mixes
    reactions:
      base:
        > Reagent   Volume
        > =======  =======
        > a           1 µL
        > b           2 µL
        > c           3 µL
        > d           4 µL
      combos:
        -
          a: 1
          b: 1
        -
          a: 2
          b: 1
        -
          a: 3
          b: 1
        -
          a: 4
          b: 1
        -
          a: 1
          b: 2
        -
          a: 2
          b: 2
        -
          a: 3
          b: 2
        -
          a: 4
          b: 2
    
    expected:
      -
        > pl(
        >   "Setup 8 reactions:",
        >   ul(
