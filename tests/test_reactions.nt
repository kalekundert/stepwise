        >         ],
        >         footer=["", "6.00 µL", "12.00 µL"],
        >         align=list("<>>"),
        >       ),
        >     ),
        >     br='\n\n',
        >   ),
        >   ul("Split into 2 identical 6.00 µL reactions."),
        > )
  -
    id: 2-mixes-instructions
    reactions:
      base:
        > Reagent   Volume
        > =======  =======
        > a           1 µL
        > b           2 µL
        > c           3 µL
      combos:
        -
          a: 1
        -
          a: 2
      exec:
        > rxns.instructions += 'd'
        > rxns.instructions += 'e'
    
    expected:
      -
        > pl(
        >   "Setup 2 reactions:",
        >   ul(
        >     pl(
        >       "Use the following reagents:",
        >       dl(('a', '1, 2')),
        >     ),
        >     pl(
        >       "Make master mix:",
        >       table(
        >         header=["Reagent", "Volume", "2x"],
        >         rows=[
