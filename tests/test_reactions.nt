        >         header=["Reagent", "Volume", "8x"],
        >         rows=[
        >             ["b", "2.00 µL", "16.00 µL"],
        >             ["master mix", "7.00 µL", "56.00 µL"],
        >         ],
        >         footer=["", "9.00 µL", "72.00 µL"],
        >         align=list("<>>"),
        >       ),
        >     ),
        >     pl(
        >       "Setup 8 reactions:",
        >       table(
        >         header=["Reagent", "Volume", "2x"],
        >         rows=[
