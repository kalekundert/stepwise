        >       "Setup the reactions:",
        >       table(
        >         header=["Reagent", "Volume"],
        >         rows=[
        >             ["a", "1.00 µL"],
        >             ["master mix", "5.00 µL"],
        >         ],
        >         footer=["", "6.00 µL"],
