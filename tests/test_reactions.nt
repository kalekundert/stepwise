    id: ab
    args:
      --combo-reagents: x,y
      --combo:
        - a,b
    expected:
      -
        x: a
        y: b
  -
    id: ab-cd
    args:
      --combo-reagents: x,y
      --combo:
        - a,b
        - c,d
    expected:
      -
        x: a
        y: b
      -
        x: c
        y: d
  -
    id: err-wrong-number-of-reagents
    args:
      --combo-reagents: x,y
      --combo:
        - a,b
        - c,d,e
    error:
      type: UsageError
      message: expected 2 reagents, not 3: c,d,e
  -
    id: err-header-only
    args:
      --combo-reagents: x
    error:
      type: UsageError
      message: specified combo reagents (`-C`), but no combos (`-c`)

test_extra_from_docopt:
  -
    id: empty
    args:
      {}
    error: KeyError
  -
    id: fraction
    args:
      --extra-fraction: 0.1
    expected:
      > Extra(fraction=0.1)
  -
    id: percent
    args:
      --extra-percent: 10
    expected:
      > Extra(fraction=0.1)
  -
    id: reactions
    args:
      --extra-reactions: 1
    expected:
      > Extra(reactions=1)
  -
    id: volume
    args:
      --extra-volume: 5
    expected:
      > Extra(volume=5)
  -
    id: min-volume
    args:
      --extra-min-volume: 0.5
    expected:
      > Extra(min_volume=0.5)

