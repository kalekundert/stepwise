        >             ["a", "1.00 µL"],
        >             ["b mix", "9.00 µL"],
        >         ],
        >         footer=["", "10.00 µL"],
        >         align=list("<>"),
        >       ),
        >     ),
        >     br='\n\n',
        >   ),
        > )
  -
    id: 3-mixes-2-replicates
    reactions:
      base:
        > Reagent   Volume
        > =======  =======
        > a           1 µL
        > b           2 µL
        > c           3 µL
        > d           4 µL
      combos:
        -
          a: 1
          b: 1
        -
          a: 2
          b: 1
        -
          a: 3
          b: 1
        -
          a: 4
          b: 1
        -
          a: 1
          b: 2
        -
          a: 2
          b: 2
        -
          a: 3
          b: 2
        -
