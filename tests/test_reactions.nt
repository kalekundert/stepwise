  -
    id: step-kind
    reactions:
      base:
        > Reagent   Volume
        > =======  =======
        > a           1 µL
      combos:
        []
      exec:
        > rxns.step_kind = 'test'
    
    expected:
      -
        > pl(
        >   "Setup 1 test reaction:",
        >   table(
        >     header=["Reagent", "Volume"],
        >     rows=[
